{
    "name": "Bookmark Dashboard",
    "description": "Acts as a reading list for bookmarks",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["bookmarks"],
    "action":{
        "default_popup": "popup.html"
    }
}